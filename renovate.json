{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true,
      "automergeSchedule": "after 11:30pm and before 5:00am"
      "ignoreTests": true, // I'll probably regret this
    }
  ]
}
